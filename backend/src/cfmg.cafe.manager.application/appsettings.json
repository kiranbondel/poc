{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.LifeTime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppId": "Cfmg.Cafe.Manager",
  "Database": {
    "ConnectionString": "Server=${SQL_SERVER};Database=${DB_NAME};User Id=${DB_USER};Password=${DB_PASSWORD};TrustServerCertificate=True;",
    "MaxRetryCount": 3,
    "MaxRetryDelaySeconds": 30
  },
  "Migration": {
    "Enable": "false"
  },
  "SeedData": {
    "Enable": "false"
  }
}
